{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    },
    "commands": {
      "build": "npm run build"
    },
    "outputDirectory": "dist"
  }
}
